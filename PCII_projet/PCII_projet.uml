        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Uz7vQIscEeqC4q-6vJG--A" name="Piste" specification="_UwKpkIscEeqC4q-6vJG--A">
          <ownedParameter xmi:id="_Uz7vQYscEeqC4q-6vJG--A" type="_UwJbcIscEeqC4q-6vJG--A" direction="return">
            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>this.trackL = new ArrayList&lt;Point>();
		this.trackR= new ArrayList&lt;Point>();

		this.obsList = new ArrayList&lt;Obstacle>();
		this.oppList = new ArrayList&lt;Opponent>();
		
		this.trackSize = 0;
		this.traveledDist = 0;
		this.score = 0;
		
		this.traveledSinceCP = 0;
	</body>
