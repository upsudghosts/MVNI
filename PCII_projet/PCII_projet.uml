          <language>java</language>
          <body>for(int i = 0; i &lt; imgBg.size(); i++){
 		BufferedImage I = imgBg.get(i);
 		if (i == 0) {
 			g2d.drawImage(
     				I, 
     				0,0,
     				WIDTH, horHeight-2, 
     				null);
 		} else {
 			int imageW = I.getWidth();
 			// Tile the image to fill our area.
 	        for (int x = -10*WIDTH; x &lt; 10*WIDTH; x += imageW) {
	    	        g2d.drawImage(
	    	        		I, 
	    	        		x + mvBg[i-1], 0,
	    	        		this);
 	        }
 		}
 	}
 </body>
        </ownedBehavior>
